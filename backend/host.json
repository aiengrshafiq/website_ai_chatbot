{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "gunicorn",
      "workingDirectory": "",
      "arguments": [
        "-w", "4",
        "-k", "uvicorn.workers.UvicornH11Worker",
        "--bind", "0.0.0.0:%FUNCTIONS_CUSTOMHANDLER_PORT%",
        "main:app"
      ]
    },
    "enableForwardingHttpRequest": true
  }
}